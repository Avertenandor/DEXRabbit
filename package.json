{
  "name": "dexrabbit",
  "version": "1.0.0",
  "description": "DEXRabbit - Платформа для токенизированного разведения кроликов",
  "main": "index.html",
  "scripts": {
    "format": "prettier -w .",
    "lint:html": "htmlhint \"**/*.html\"",
    "lint:css": "stylelint \"assets/css/**/*.css\"",
    "lint:js": "eslint \"assets/js/**/*.js\"",
    "links": "linkinator http://127.0.0.1:8080 --recurse --concurrency 10",
    "opt:img": "imagemin assets/img/* --plugin=mozjpeg --plugin=pngquant > /dev/null || true",
    "test": "npm run lint:html && npm run lint:css && npm run lint:js",
    "prepare": "husky"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Avertenandor/DEXRabbit.git"
  },
  "keywords": [
    "crypto",
    "investment",
    "rabbits",
    "blockchain",
    "PLEX"
  ],
  "author": "DEXRabbit Team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Avertenandor/DEXRabbit/issues"
  },
  "homepage": "https://xn--80apagbbfxgmuj4j.site/",
  "devDependencies": {
    "prettier": "^3.0.0",
    "eslint": "^8.50.0",
    "stylelint": "^15.10.0",
    "htmlhint": "^1.1.4",
    "linkinator": "^5.0.0",
    "lighthouse": "^11.0.0",
    "husky": "^8.0.0",
    "lint-staged": "^14.0.0",
    "imagemin": "^8.0.0",
    "imagemin-mozjpeg": "^10.0.0",
    "imagemin-pngquant": "^9.0.0"
  }
}
